		\path[fill=white] (-\cardwidth/2 - \bleed, 0in) --%
		      (-\cardwidth/2 - \bleed, \cardheight/2 + \bleed) --%
		      (\cardwidth/2 + \bleed, \cardheight/2 + \bleed) --%
		      (\cardwidth/2 + \bleed, -\cardheight/2 - \bleed) --%
		      (-\cardwidth/2 - \bleed, -\cardheight/2 - \bleed) --%
		      cycle;

		% Pattern for card front
